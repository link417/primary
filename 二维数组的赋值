#include<stdio.h> 
int main()
{
	int a[2][3],b[3][2];
	int max,min;
	int h,i,j,l;
	for(i=0;i<2;i++)
	{
		for(j=0;j<3;j++)
		{
		printf("a[%d][%d]=",i,j);
		scanf("%d",&a[i][j]);
       	}
	}
	    printf("输出二维数组： \n");
		for(i=0;i<2;i++)
		{
			for(j=0;j<3;j++){
				printf("%d\n",a[i][j]);
				
			}
		 } 
		 printf("\n");
	/*求数组中最大元素及其下标*/
	 
	return 0;
	
}
