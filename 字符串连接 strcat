#include<stdio.h>
#include<string.h>
int main()
{
	char str1[50];
	char str2[50];
	printf("please input str1\n");
	gets(str1);
	printf("please input str2\n");
	gets(str2);
	printf("str1:");
	puts(str1);
	printf("\n");
	printf("str2:");
	puts(str2);
	
	
	strcat(str1,str2);
	printf("the new words are:");
	puts(str1);
	return 0;
	
 } 
