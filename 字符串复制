#include<stdio.h>
#include<string.h>
int main()
{
	char str1[50];
	char str2[50];
	printf("please input str1\n");
	gets(str1);
	printf("please input str2\n");
	gets(str2);
	printf("str1:");
	puts(str1);
	printf("\n");
	printf("str2:");
	puts(str2);
	
	strcpy(str2,str1);
	printf("str2 are insteaded of str1\n the new str2:");
	puts(str2);
	return 0;
	
}
