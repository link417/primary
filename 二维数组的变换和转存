#include<stdio.h>
int main()
{
	int a[2][3],b[3][2];
	int i,j,h,l;
	printf("请输出数字\n");


    for(i=0;i<2;i++){
    	for(j=0;j<3;j++){
    		printf("a[%d][%d]=",i,j);
    		scanf("%d",&a[i][j]);
    		
		}
	}
	/*将数组a转换后存入数组b*/
	for(i=0;i<2;i++){
		for(j=0;j<3;j++){
			b[j][i]=a[i][j];
				
		}

	}
	
	 printf("输出转换后的二维数组： \n");
	 for(i=0;i<3;i++){
	 	for(j=0;j<2;j++){
	 		printf("b[%d][%d]=%d\t",j,i,b[i][j]);
		 }
		 printf("\n");
	 }
	
	
	
	return 0;
	
	
}
